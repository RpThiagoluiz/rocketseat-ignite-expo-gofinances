{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src/",
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES6",
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "extends": "expo/tsconfig.base"
  // "include": ["./src/**/*.ts", "./src/**/*.d.ts", "src/components/testHeader/index.tsx"]
}
